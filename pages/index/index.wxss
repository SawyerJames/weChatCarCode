/*页脚样式  */
@import "../footer-template/footerTemplate.wxss";

/*背景  */

page {
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100%;
}

/*通知公告  */

.noticeContainer {
    width: 100%;
    height: 60rpx;
    color: #fff;
    display: flex;
    align-items: center;
    box-sizing: border-box;
    padding: 0 5%;
    font-size: 24rpx;
}

.notice_icon {
    width: auto;
    height: 100%;
    display: flex;
    align-items: center;
}

.notice_icon image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
}

.noticeSwiper {
    height: 100%;
    flex: 1;
}

.noticeSwiper swiper-item {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
}

.noticeSwiper swiper-item text {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

/**用户信息**/

.userinfo {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 150rpx 0 0rpx;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
    box-shadow: 1px 1px 5px 1px #ddd;
}

.userinfo-nickname {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #3e99e2;
    font-size: 32rpx;
    font-family: sans-serif;
}

.usermotto {
    margin-top: 200px;
}

/*广告位  */

.advertContainer {
    width: 90%;
    height: 400rpx;
    margin: 40rpx 0 50rpx;
}

.advertSwiper {
    width: 100%;
    height: 100%;
}

.advertSwiper swiper-item image {
    width: 100%;
    height: 100%;
    border-radius: 6rpx;
}

/*快速乘车  */

.rideCarContainer {
    width: 90%;
    height: 100rpx;
    display: flex;
    color: #fff;
    font-size: 32rpx;
    box-sizing: border-box;
    border-radius: 4rpx;
    padding: 0 5%;
    align-items: center;
    justify-content: space-between;
    background: url("data:image/png;base64,");
    background-size: cover;
}

.rideCarContainer view {
    display: flex;
    align-items: center;
}

.rideCarContainer .rideCarIcon {
    width: 80rpx;
    height: 70rpx;
    margin-right: 30rpx;
}

.rideCarContainer .arrow {
    width: 30rpx;
    height: 30rpx;
}
